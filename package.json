{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "engines": {
    "node": "^18.19.0 || ^20.9.0 || >=22.11.0"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://npm.im/@travi/cli",
  "bin": {
    "travi": "bin/travi.js"
  },
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:engines": "ls-engines",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any vitest run",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "husky install",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "pretest:e2e": "run-s build",
    "test:e2e": "node ./test/e2e/smoke-test.js",
    "lint:publish": "publint --strict",
    "lint:js:fix": "run-s 'lint:js -- --fix'",
    "lint:lockfile": "lockfile-lint"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "npm@11.1.0+sha512.acf301ad9b9ddba948fcb72341e2f0fcae477f56a95cc2a092934d133a7461062633cefbf93d5934a3dc0768674e2edee9f04dcfcc4bb4c327ff0e3a7d552a1b",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/add-package-to-monorepo": "12.0.3",
    "@form8ion/core": "4.8.0",
    "@form8ion/cucumber-scaffolder": "4.0.0",
    "@form8ion/cypress-scaffolder": "3.0.3",
    "@form8ion/dependabot-scaffolder": "3.0.0",
    "@form8ion/eslint-config-extender": "10.0.3",
    "@form8ion/github": "2.1.1",
    "@form8ion/github-actions-node-ci": "5.0.1",
    "@form8ion/hapi-scaffolder": "5.0.0-alpha.6",
    "@form8ion/javascript": "14.1.0",
    "@form8ion/javascript-core": "12.3.0",
    "@form8ion/jest-scaffolder": "1.2.2",
    "@form8ion/jetbrains": "1.1.3",
    "@form8ion/lerna": "3.0.0-beta.4",
    "@form8ion/lift": "11.0.0",
    "@form8ion/mdx-deck": "1.1.2",
    "@form8ion/mocha-scaffolder": "6.0.0-beta.8",
    "@form8ion/nuxt": "2.0.0",
    "@form8ion/octoherd-script": "1.0.0",
    "@form8ion/ossf-scorecard": "1.2.1",
    "@form8ion/php": "1.9.1",
    "@form8ion/probot-scaffolder": "1.0.0-alpha.5",
    "@form8ion/project": "21.0.0-alpha.1",
    "@form8ion/react-components-scaffolder": "3.0.3",
    "@form8ion/remove-greenkeeper": "3.0.1",
    "@form8ion/renovate-scaffolder": "3.0.0",
    "@form8ion/replace-travis-ci-with-github-actions": "10.0.1",
    "@form8ion/rollup": "6.0.0-beta.1",
    "@form8ion/ruby-scaffolder": "2.1.0",
    "@form8ion/scaffolder-scaffolder": "7.0.1",
    "@form8ion/shuttle": "1.0.2",
    "@form8ion/ski-patrol": "1.0.0-alpha.5",
    "@form8ion/slidev": "3.0.0-alpha.1",
    "@form8ion/sno-cat-ink": "1.0.0-alpha.3",
    "@form8ion/vite": "2.0.1",
    "@form8ion/vitest": "5.0.0",
    "@travi/circle-scaffolder-javascript": "1.2.2",
    "@travi/gitlab-scaffolder": "1.1.1",
    "@travi/netlify-scaffolder": "1.6.0",
    "@travi/node-app-engine-standard-scaffolder": "2.0.1",
    "@travi/shell-scaffolder": "2.0.2",
    "@travi/spectacle-scaffolder": "5.0.0",
    "@travi/travis-scaffolder-shell": "1.0.0-alpha.4",
    "commander": "13.1.0",
    "inquander": "1.1.1",
    "snyk": "^1.355.0",
    "update-notifier": "7.3.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "11.2.0",
    "@form8ion/config-file": "1.3.0",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@travi/any": "3.1.2",
    "@travi/eslint-config": "5.0.11",
    "@travi/eslint-config-cucumber": "1.0.6",
    "c8": "10.1.3",
    "chai": "5.1.2",
    "clear-module": "4.1.2",
    "commitlint-config-travi": "1.4.55",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "deep-equal": "2.2.3",
    "execa": "9.5.2",
    "gherkin-lint": "4.2.4",
    "globstar": "1.0.0",
    "http-status-codes": "2.3.0",
    "husky": "9.1.7",
    "import-fresh": "3.3.1",
    "jest-when": "3.7.0",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.14.0",
    "ls-engines": "0.9.3",
    "mdast-util-from-markdown": "2.0.2",
    "mock-fs": "5.5.0",
    "msw": "2.7.0",
    "npm-run-all2": "7.0.2",
    "publint": "0.3.4",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.7",
    "remark-toc": "9.0.0",
    "rimraf": "6.0.1",
    "rollup": "4.34.6",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.3",
    "rollup-plugin-json": "4.0.0",
    "sinon": "19.0.2",
    "testdouble": "3.20.2",
    "unist-util-find": "3.0.0",
    "vitest": "3.0.5"
  },
  "snyk": true
}
