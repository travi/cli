{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://npm.im/@travi/cli",
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "lint:engines": "check-engine",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/node": "7.12.6",
    "@babel/register": "7.12.1",
    "@travi/any": "2.0.15",
    "@travi/babel-preset": "3.0.36",
    "@travi/eslint-config": "1.0.56",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "check-engine": "1.10.0",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.3.0",
    "doctoc": "2.0.0",
    "gherkin-lint": "4.1.3",
    "globstar": "1.0.0",
    "http-status-codes": "2.1.4",
    "husky": "4.3.4",
    "markdownlint-cli": "0.25.0",
    "mocha": "8.2.1",
    "mock-fs": "4.13.0",
    "mz": "2.7.0",
    "nock": "13.0.5",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.34.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-executable": "1.6.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.2.1",
    "testdouble": "3.16.1"
  },
  "dependencies": {
    "@form8ion/add-package-to-monorepo": "1.3.0",
    "@form8ion/cucumber-scaffolder": "1.4.0",
    "@form8ion/cypress-scaffolder": "2.2.0",
    "@form8ion/dependabot-scaffolder": "1.2.0",
    "@form8ion/eslint-config-extender": "1.1.0",
    "@form8ion/github-actions-node-ci": "1.10.1",
    "@form8ion/hapi-scaffolder": "2.0.2",
    "@form8ion/javascript-core": "2.7.1",
    "@form8ion/jest-scaffolder": "1.0.0",
    "@form8ion/lift": "1.10.3",
    "@form8ion/lift-javascript": "1.7.1",
    "@form8ion/mdx-deck": "1.1.0",
    "@form8ion/mocha-scaffolder": "1.0.0",
    "@form8ion/nuxt": "1.0.0",
    "@form8ion/probot-scaffolder": "1.0.0-alpha.5",
    "@form8ion/react-components-scaffolder": "2.2.1",
    "@form8ion/remove-greenkeeper": "1.1.7",
    "@form8ion/renovate-scaffolder": "1.3.0",
    "@form8ion/replace-travis-ci-with-github-actions": "1.2.5",
    "@form8ion/ruby-scaffolder": "1.0.4",
    "@form8ion/shuttle": "1.0.0-alpha.5",
    "@form8ion/ski-patrol": "1.0.0-alpha.5",
    "@form8ion/sno-cat-ink": "1.0.0-alpha.2",
    "@travi/circle-scaffolder-javascript": "1.2.1",
    "@travi/github-scaffolder": "5.3.0",
    "@travi/gitlab-scaffolder": "1.0.4",
    "@travi/javascript-scaffolder": "11.16.0",
    "@travi/netlify-scaffolder": "1.6.0",
    "@travi/node-app-engine-standard-scaffolder": "1.2.5",
    "@travi/project-scaffolder": "8.10.12",
    "@travi/shell-scaffolder": "1.1.2",
    "@travi/spectacle-scaffolder": "2.5.4",
    "@travi/travis-scaffolder-javascript": "4.0.0",
    "@travi/travis-scaffolder-shell": "1.0.0-alpha.4",
    "commander": "6.2.0",
    "inquander": "1.1.1",
    "snyk": "^1.355.0",
    "travis-token-updater": "1.6.12",
    "update-notifier": "5.0.1"
  },
  "snyk": true
}
