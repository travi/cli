{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://npm.im/@travi/cli",
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha --recursive test/unit",
    "test": "run-s lint:* test:* build",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/node": "7.4.5",
    "@babel/register": "7.4.4",
    "@travi/any": "2.0.0",
    "@travi/babel-preset": "3.0.0",
    "@travi/eslint-config-travi": "1.8.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.5.0",
    "commitlint-config-travi": "1.3.0",
    "cz-conventional-changelog": "2.1.0",
    "doctoc": "1.4.0",
    "globstar": "1.0.0",
    "husky": "2.4.1",
    "markdownlint-cli": "0.17.0",
    "mocha": "6.1.4",
    "mz": "2.7.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "rollup": "1.16.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-executable": "1.4.6",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.0.3",
    "sinon": "7.3.2"
  },
  "dependencies": {
    "@travi/scaffolder-sub-command": "1.17.4",
    "commander": "2.20.0",
    "inquander": "1.1.1",
    "travis-token-updater": "1.0.0-alpha.17",
    "update-notifier": "3.0.0"
  }
}
