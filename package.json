{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://npm.im/@travi/cli",
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha --recursive test/unit",
    "test": "run-s lint:* test:* build",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.5",
    "@travi/eslint-config-travi": "1.6.18",
    "babel-preset-travi": "1.3.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.0.4",
    "commitlint-config-travi": "1.2.30",
    "cz-conventional-changelog": "2.1.0",
    "doctoc": "1.3.1",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.15.1",
    "husky": "1.0.0-rc.9",
    "markdownlint-cli": "0.11.0",
    "mocha": "5.2.0",
    "mz": "2.7.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "rollup": "0.61.2",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-executable": "1.2.0",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "sinon": "6.0.1"
  },
  "dependencies": {
    "@travi/scaffolder-sub-command": "1.0.46",
    "commander": "2.16.0",
    "inquander": "1.1.1",
    "update-notifier": "2.5.0"
  }
}
