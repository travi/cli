{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://github.com/travi/cli#readme",
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.6.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chai": "4.1.2",
    "commitlint-config-travi": "1.2.19",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.13.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.6.0",
    "mocha": "4.1.0",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "rollup": "0.54.1",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-json": "2.3.0"
  },
  "dependencies": {
    "commander": "2.13.0",
    "update-notifier": "2.3.0"
  }
}
