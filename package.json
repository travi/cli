{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://npm.im/@travi/cli",
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:engines": "check-engine",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test": "run-s lint:* test:* build",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "generate:readme": "doctoc README.md --entryprefix '*'",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.9.0",
    "@travi/any": "2.0.8",
    "@travi/babel-preset": "3.0.19",
    "@travi/eslint-config": "1.0.34",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "check-engine": "1.8.1",
    "codecov": "3.6.5",
    "commitlint-config-travi": "1.3.6",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.2.0",
    "doctoc": "1.4.0",
    "gherkin-lint": "4.0.1",
    "globstar": "1.0.0",
    "http-status-codes": "1.4.0",
    "husky": "4.2.5",
    "markdownlint-cli": "0.23.0",
    "mocha": "7.1.2",
    "mock-fs": "4.12.0",
    "mz": "2.7.0",
    "nock": "12.0.3",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.9.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-executable": "1.6.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.0.2",
    "testdouble": "3.13.1",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "@form8ion/cucumber-scaffolder": "1.1.0",
    "@form8ion/lift": "1.9.2",
    "@form8ion/lift-javascript": "1.2.0",
    "@form8ion/remove-greenkeeper": "1.1.4",
    "@form8ion/renovate-scaffolder": "1.3.0",
    "@form8ion/ski-patrol": "1.0.0-alpha.3",
    "@travi/scaffolder-sub-command": "1.23.3",
    "commander": "5.1.0",
    "inquander": "1.1.1",
    "snyk": "1.320.2",
    "travis-token-updater": "1.6.12",
    "update-notifier": "4.1.0"
  },
  "snyk": true
}
