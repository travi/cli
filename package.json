{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://github.com/travi/cli#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "commitmsg": "commitlint -e",
    "precommit": "npm test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.6.0",
    "chai": "4.1.2",
    "commitlint-config-travi": "1.2.19",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.6.0",
    "mocha": "4.1.0",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1"
  }
}
