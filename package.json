{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://github.com/travi/cli#readme",
  "scripts": {
    "clean": "rimraf bin/",
    "copy:project-templates": "cpy 'templates/**/*.*' '../../' --parents --cwd=packages/project",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha --recursive test/unit",
    "test:project-scaffolder": "mocha --recursive packages/project/test/unit",
    "test:integration": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test": "run-s lint:* test:* build",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "build:js": "rollup -c",
    "build": "run-s clean build:* copy:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "semantic-release": "semantic-release",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.2",
    "@travi/eslint-config-travi": "1.6.13",
    "babel-core": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "bdd-stdin": "0.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.0.0",
    "commitlint-config-travi": "1.2.29",
    "cpy-cli": "1.0.1",
    "cucumber": "4.2.1",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.14.0",
    "husky": "0.15.0-rc.13",
    "markdownlint-cli": "0.8.1",
    "mocha": "5.1.0",
    "mock-fs": "4.4.2",
    "npm-run-all": "4.1.2",
    "nyc": "11.6.0",
    "rimraf": "2.6.2",
    "rollup": "0.58.0",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-executable": "1.2.0",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "semantic-release": "15.1.7",
    "sinon": "4.5.0"
  },
  "dependencies": {
    "@travi/javascript-scaffolder": "1.0.0",
    "chalk": "2.3.2",
    "commander": "2.15.1",
    "cp-file": "5.0.0",
    "inquander": "1.1.1",
    "inquirer": "5.2.0",
    "lodash.uniq": "4.5.0",
    "mustache": "2.3.0",
    "mz": "2.7.0",
    "nodegit": "0.22.0",
    "npm-conf": "1.1.3",
    "shelljs": "0.8.1",
    "spdx-license-list": "4.0.0",
    "update-notifier": "2.5.0",
    "write-yaml": "1.0.0"
  }
}
