{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://github.com/travi/cli#readme",
  "scripts": {
    "clean": "rimraf bin/",
    "copy:templates": "cpy '**/*.*' '../../bin/' --parents --cwd=src/scaffold-project",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "tests:unit": "mocha --recursive test/unit",
    "tests:integration": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test": "run-s lint:* coverage build",
    "coverage": "nyc run-s tests:unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "build:js": "rollup -c",
    "build": "run-s clean build:* copy:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "semantic-release": "semantic-release",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "bin": {
    "travi": "bin/travi.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.2",
    "@travi/eslint-config-travi": "1.6.7",
    "babel-core": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "bdd-stdin": "0.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "codecov": "3.0.0",
    "commitlint-config-travi": "1.2.27",
    "cpy-cli": "1.0.1",
    "cucumber": "4.0.0",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.13.2",
    "husky": "0.15.0-rc.8",
    "markdownlint-cli": "0.7.1",
    "mocha": "5.0.1",
    "mock-fs": "4.4.2",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "rollup": "0.56.2",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-executable": "1.2.0",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "semantic-release": "12.4.1",
    "sinon": "4.3.0"
  },
  "dependencies": {
    "chalk": "2.3.0",
    "commander": "2.14.1",
    "cp-file": "5.0.0",
    "inquander": "1.1.1",
    "inquirer": "5.1.0",
    "mustache": "2.3.0",
    "mz": "2.7.0",
    "nodegit": "0.20.3",
    "shelljs": "0.8.1",
    "spdx-license-list": "4.0.0",
    "update-notifier": "2.3.0"
  }
}
