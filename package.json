{
  "name": "@travi/cli",
  "description": "cli for my various tools",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/cli",
  "bugs": "https://github.com/travi/cli/issues",
  "homepage": "https://npm.im/@travi/cli",
  "bin": {
    "travi": "bin/travi.js"
  },
  "engines": {
    "node": "^14.18 || ^16.0.0"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/node": "7.18.10",
    "@babel/register": "7.18.9",
    "@cucumber/cucumber": "8.5.1",
    "@form8ion/core": "2.0.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@travi/any": "2.1.5",
    "@travi/babel-preset": "3.0.91",
    "@travi/eslint-config": "3.0.5",
    "@travi/eslint-config-cucumber": "1.0.6",
    "@travi/eslint-config-mocha": "1.0.19",
    "c8": "7.12.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "clear-module": "4.1.2",
    "commitlint-config-travi": "1.4.16",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "globstar": "1.0.0",
    "http-status-codes": "2.2.0",
    "husky": "8.0.1",
    "import-fresh": "3.3.0",
    "js-yaml": "4.1.0",
    "ls-engines": "0.7.0",
    "mdast-util-from-markdown": "0.8.5",
    "mocha": "10.0.0",
    "mock-fs": "5.1.4",
    "nock": "13.2.9",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "4.0.1",
    "remark-toc": "8.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.78.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-executable": "1.6.3",
    "rollup-plugin-json": "4.0.0",
    "sinon": "14.0.0",
    "testdouble": "3.16.6",
    "unist-util-find": "1.0.2"
  },
  "dependencies": {
    "@form8ion/add-package-to-monorepo": "6.0.3",
    "@form8ion/cucumber-scaffolder": "2.1.0",
    "@form8ion/cypress-scaffolder": "3.0.0",
    "@form8ion/dependabot-scaffolder": "1.4.1",
    "@form8ion/eslint-config-extender": "4.0.2",
    "@form8ion/github-actions-node-ci": "3.8.2",
    "@form8ion/hapi-scaffolder": "4.0.5",
    "@form8ion/javascript": "8.0.2",
    "@form8ion/javascript-core": "7.0.1",
    "@form8ion/jest-scaffolder": "1.2.1",
    "@form8ion/lerna": "2.1.1",
    "@form8ion/lift": "4.1.0",
    "@form8ion/mdx-deck": "1.1.1",
    "@form8ion/mocha-scaffolder": "2.0.2",
    "@form8ion/nuxt": "1.0.0",
    "@form8ion/probot-scaffolder": "1.0.0-alpha.5",
    "@form8ion/project": "13.2.1",
    "@form8ion/react-components-scaffolder": "3.0.1",
    "@form8ion/remove-greenkeeper": "1.1.10",
    "@form8ion/renovate-scaffolder": "1.8.0",
    "@form8ion/replace-travis-ci-with-github-actions": "5.0.3",
    "@form8ion/rollup": "2.0.1",
    "@form8ion/ruby-scaffolder": "1.0.4",
    "@form8ion/shuttle": "1.0.0-alpha.5",
    "@form8ion/ski-patrol": "1.0.0-alpha.5",
    "@form8ion/slidev": "1.0.1",
    "@form8ion/sno-cat-ink": "1.0.0-alpha.3",
    "@form8ion/vite": "1.2.0",
    "@form8ion/vitest": "1.1.0",
    "@travi/circle-scaffolder-javascript": "1.2.1",
    "@travi/github-scaffolder": "8.0.2",
    "@travi/gitlab-scaffolder": "1.0.5",
    "@travi/netlify-scaffolder": "1.6.0",
    "@travi/node-app-engine-standard-scaffolder": "2.0.0",
    "@travi/shell-scaffolder": "1.1.4",
    "@travi/spectacle-scaffolder": "4.0.1",
    "@travi/travis-scaffolder-shell": "1.0.0-alpha.4",
    "commander": "9.4.0",
    "inquander": "1.1.1",
    "snyk": "^1.355.0",
    "travis-token-updater": "1.6.14",
    "update-notifier": "5.1.0"
  },
  "snyk": true,
  "scripts": {
    "clean": "rimraf bin/",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:engines": "ls-engines",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "mocha 'src/**/*-test.js'",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "generate:md": "remark . --output",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "husky install",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'"
  }
}
